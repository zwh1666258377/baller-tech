{
  "name": "baller-tech",
  "script": "./server/dist/run.js",
  "exec_interpreter": "./node_modules/.bin/babel-node",
  "exec_mode": "fork",
  "error_file": "./logs/error.log",
  "out_file": "./logs/console.log",
  "pid_file": "./logs/pid_file.pid",
